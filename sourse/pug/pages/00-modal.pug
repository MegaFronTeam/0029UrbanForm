extends ../layout/main.pug 
include ../blocks/sForm/_sForm.pug 
block variables
	- let  mainPage= 'main-page' 
	- let  bg= null

block title
	- let  title= index.head.title
	title= title
block pagecontent
	//- +mob-mnu()
	
	
	+parts('header')
	main
		.section.container
			h1 Modal page 
			mixin btn(a, t)
				a(data-fancybox data-type="ajax" href= a).btn.btn-primary= t
			+btn("modal/policy-modal.html", 'policy-modal')
			+btn("modal/thanks.html", 'thanks')

	+parts('footer')
      



